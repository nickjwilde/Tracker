<div class="container">
<div class="row">
<div class="col-sm-6">
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
			</tr>
		</thead>
		<tbody>
			<% @userlist.each do | user | %>
				<tr>
					<td><%= user.get_first_name %></td>
					<td><%= user.get_last_name %></td>
					<td><%= user.get_email %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
</div>
</div>
